snippet helloworld "Basic hello world program"
	fn main() {
		println!("Hello, world!");
	}
snippet pt
	print!("${1:Hello, world!}"$2);
snippet ptln
	println!("${1:Hello, world!}"$2);
snippet ptlnvar
	println!("${2:$1}: {${3::#?}}", ${1:var});
snippet ept
	eprint!("${1:Hello, world!}"$2);
snippet eptln
	eprintln!("${1:Hello, world!}"$2);
snippet eptlnvar
	eprintln!("${2:$1}: {${3::#?}}", ${1:var});
